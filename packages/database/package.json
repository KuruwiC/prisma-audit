{
  "name": "@kuruwic/prisma-audit-database",
  "version": "0.1.0",
  "type": "module",
  "description": "Shared Prisma database schema and client for prisma-audit monorepo",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./generated/client": {
      "types": "./generated/client/index.d.ts",
      "import": "./generated/client/index.js",
      "require": "./generated/client/index.js"
    }
  },
  "files": [
    "dist",
    "generated",
    "prisma/schema.prisma",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup && tsc",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0"
  },
  "devDependencies": {
    "@kuruwic/prisma-audit-typescript-config": "workspace:*",
    "prisma": "^6.19.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
